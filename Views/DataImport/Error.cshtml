
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ImportDataFromExcel</title>
    <link rel="icon" href="@Url.Content("~/content/ImportDataFromExcelFavicon.ico")" />

    <link rel="stylesheet" href="~/Content/css/bootstrap.min.css" crossorigin="anonymous">
    <link href="~/Content/css/fileinput.css" media="all" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.5.0/css/all.css" crossorigin="anonymous">
    <!-- <link rel="stylesheet" href="~/Content/css/all.css" crossorigin="anonymous"> -->
    <link href="~/Content/css/theme.css" media="all" rel="stylesheet" type="text/css">
    <!--<script src="https://code.jquery.com/jquery-3.3.1.min.js" crossorigin="anonymous"></script>-->
    <script src="~/Content/js/jquery-3.3.1.min.js" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>
    <!-- <script src="~/Content/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script> -->
    <script src="~/Content/js/piexif.js" type="text/javascript"></script>
    <script src="~/Content/js/sortable.js" type="text/javascript"></script>
    <script src="~/Content/js/fileinput.js" type="text/javascript"></script>
    <script src="~/Content/js/fr.js" type="text/javascript"></script>
    <script src="~/Content/js/es.js" type="text/javascript"></script>
    <script src="~/Content/js/themes/fas/theme.js" type="text/javascript"></script>
    <script src="~/Content/js/themes/explorer-fas/theme.js" type="text/javascript"></script>


    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Content/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/css/animate/animate.css">
    <link rel="stylesheet" type="text/css" href="~/Content/css/select2.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/css/animate/perfect-scrollbar.css">
    <link rel="stylesheet" type="text/css" href="~/Content/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/Content/css/main.css">
    <!--===============================================================================================-->


    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

    <style>
        table, tr, th, td {
            border: 1px solid black;
        }
    </style>

</head>
<body>
    <div class="outerDiv card">
        <div class="container my-4 mainDiv" style="width: auto;">
            <div class="card-header">
                Data Import Web API
            </div>
            <div class="card-body">
                <div class="" style="text-align: center;">
                    <img src="~/content/error.png" class="statusImg">
                    <br>
                    <span class="importStatus">Error!</span>
                    <br>
                    <span class="importStatusMessage">The data import failed!<br></span>
                </div>

                <div class="container-table100" style="margin-top: 20px;">
                    <div class="wrap-table100">
                        <div class="isa_error">
                            <span>Record in @ViewBag.Results.ReferenceId failed with message: @ViewBag.Results.MessageError</span>
                        </div>

                        <div class="table100">
                            <table class="overviewTable">
                                <thead>
                                    <tr class="table100-head" style="background-color: #e24c4b; font-weight: bold;">
                                        <th class="column1">Status</th>
                                        <th class="column2">Object</th>
                                        <th class="column3">Record Created</th>
                                        <th class="column4">Record Failed</th>
                                        <th class="column5">Start Date</th>
                                        <th class="column6">Processing Time (s)</th>
                                    </tr>
                                </thead>
                                <tbody style="background: white;">
                                    <tr>
                                        <td class="column1">@ViewBag.Results.Status</td>
                                        <td class="column2">@ViewBag.Results.Object</td>
                                        <td class="column3">@ViewBag.Results.RecordCreated</td>
                                        <td class="column4">@ViewBag.Results.RecordFailed</td>
                                        <td class="column5">@ViewBag.Results.StartDate</td>
                                        <td class="column6">@ViewBag.Results.ProcessingTime</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
